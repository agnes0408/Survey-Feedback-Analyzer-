      "source": [
        "# Step 3: Cleaning all feedback entries\n",
        "\n",
        "cleaned_feedbacks = []\n",
        "\n",
        "for fb in feedback_data['Feedback']:\n",
        "\n",
        "    # Convert to lowercase\n",
        "    fb = fb.lower()\n",
        "\n",
        "    # Remove punctuation\n",
        "    fb = fb.replace(\".\", \"\")\n",
        "    fb = fb.replace(\",\", \"\")\n",
        "    fb = fb.replace(\"!\", \"\")\n",
        "    fb = fb.replace(\"?\", \"\")\n",
        "\n",
        "    # Replace multiple spaces with single space\n",
        "    fb = \" \".join(fb.split())\n",
        "\n",
        "    # Remove leading & trailing spaces (split/join already does this)\n",
        "    fb = fb.strip()\n",
        "\n",
        "    cleaned_feedbacks.append(fb)\n",
        "\n",
        "# Update the dictionary with cleaned feedbacks\n",
        "feedback_data['Feedback'] = cleaned_feedbacks\n",
        "\n",
        "# Output cleaned feedbacks\n",
        "feedback_data['Feedback']\n"
      ],
