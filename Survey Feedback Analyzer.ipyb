        "# Step 4: Word Count Insights\n",
        "\n",
        "def count_word_in_feedbacks(word):\n",
        "    word = word.lower()  # Case-insensitive\n",
        "    count = 0\n",
        "    for fb in feedback_data['Feedback']:\n",
        "        if word in fb:   # Check if word appears\n",
        "            count += 1\n",
        "    return count\n",
        "\n",
        "# Count specific words\n",
        "good_count = count_word_in_feedbacks(\"good\")\n",
        "poor_count = count_word_in_feedbacks(\"poor\")\n",
        "excellent_count = count_word_in_feedbacks(\"excellent\")\n",
        "\n",
        "print(\"Number of feedbacks containing 'good':\", good_count)\n",
        "print(\"Number of feedbacks containing 'poor':\", poor_count)\n",
        "print(\"Number of feedbacks containing 'excellent':\", excellent_count)\n",
        "\n"
      ],
