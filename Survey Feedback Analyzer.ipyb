    {
      "cell_type": "code",
      "source": [
        "# Step 5: Final Summary & Insights\n",
        "\n",
        "# 1️⃣ Display final cleaned feedback_data\n",
        "print(\"---- Cleaned Feedback Data ----\")\n",
        "print(feedback_data)\n",
        "print(\"\\n\")\n",
        "\n",
        "\n",
        "# 2️⃣ Average Rating\n",
        "avg_rating = sum(feedback_data[\"Rating\"]) / len(feedback_data[\"Rating\"])\n",
        "print(\"Average Rating:\", round(avg_rating, 2))\n",
        "print(\"\\n\")\n",
        "\n",
        "\n",
        "# 3️⃣ Longest Feedback (by word count)\n",
        "max_words = -1\n",
        "longest_feedback = \"\"\n",
        "\n",
        "for fb in feedback_data[\"Feedback\"]:\n",
        "    word_count = len(fb.split())\n",
        "    if word_count > max_words:\n",
        "        max_words = word_count\n",
        "        longest_feedback = fb\n",
        "\n",
        "print(\"Longest Feedback:\", longest_feedback)\n",
        "print(\"Word Count:\", max_words)\n",
        "print(\"\\n\")\n",
        "\n",
        "\n",
        "# 4️⃣ Unique Words Across All Feedbacks\n",
        "unique_words = set()\n",
        "\n",
        "for fb in feedback_data[\"Feedback\"]:\n",
        "    for word in fb.split():\n",
        "        unique_words.add(word)\n",
        "\n",
        "print(\"Unique Words Used:\")\n",
        "print(sorted(unique_words))\n",
        "print(\"\\n\")\n",
        "\n",
        "\n",
        "# 5️⃣ OPTIONAL: Sort feedbacks by Rating (highest → lowest)\n",
        "\n",
        "print(\"---- Feedback Sorted by Rating (High → Low) ----\")\n",
        "sorted_list = sorted(\n",
        "    zip(feedback_data[\"Rating\"], feedback_data[\"Name\"], feedback_data[\"Feedback\"]),\n",
        "    reverse=True\n",
        ")\n",
        "\n",
        "for rating, name, fb in sorted_list:\n",
        "    print(f\"Rating: {rating} | Name: {name} | Feedback: {fb}\")\n"
      ],
